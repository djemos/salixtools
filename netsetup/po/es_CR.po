msgstr "SU ANTIGUO /etc/lilo.conf"
#
# File: 32-liloconfig, line: 1258
# File: 64-liloconfig, line: 1258
msgid "NO CONFIG FILE FOUND"
msgstr "ARCHIVO DE CONFIGURACIÓN NO ENCONTRADO"
#
# File: 32-liloconfig, line: 1258
# File: 64-liloconfig, line: 1258
msgid "Sorry, but you don't have a LILO configuration file that can be viewed."
msgstr ""
"Lo siento, pero no tiene un archivo de configuración de LILO que pueda ser "
"visto."
#
# File: 32-makepkg, line: 53
# File: 64-makepkg, line: 53
#, sh-format
msgid ""
