msgstr "Κονσόλα Frame buffer, 640x480x32k"
#
# File: 32-liloconfig, line: 193
# File: 64-liloconfig, line: 193
msgid "Frame buffer console, 800x600x32k"
msgstr "Κονσόλα Frame buffer, 800x600x32k"
#
# File: 32-liloconfig, line: 194
# File: 64-liloconfig, line: 194
msgid "Frame buffer console, 1024x768x32k"
msgstr "Κονσόλα Frame buffer, 1024x768x32k"
#
# File: 32-liloconfig, line: 195
# File: 64-liloconfig, line: 195
msgid "Frame buffer console, 640x480x256"
msgstr "Κονσόλα Frame buffer, 640x480x256"
#
# File: 32-liloconfig, line: 196
# File: 64-liloconfig, line: 196
msgid "Frame buffer console, 800x600x256"
msgstr "Κονσόλα Frame buffer, 800x600x256"
#
# File: 32-liloconfig, line: 197
# File: 64-liloconfig, line: 197
